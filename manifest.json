{
    "manifest_version": 3,
    "name": "Berkeley Id Filler",
    "version": "0.1.0",
    "action": {
        "default_popup": "popup.html",
        "default_title": "ENTER ID"
    },
    "content_scripts": [
        {
            "matches": ["https://docs.google.com/forms/*"],
            "js": ["script.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "identity",
        "tabs"
      ]
    
}